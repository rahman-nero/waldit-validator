{
    "name": "waldit/validator",
    "description": "The validator of PHP (Contribution: JoJo)",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "php": "7.4"
    },
    "autoload": {
        "psr-4": {
            "Waldit\\" : "src/waldit"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\" : "/tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test" : "vendor/bin/phpunit"
    }
}
